<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Signup</title>
    <link rel="shortcut icon" href="assets/images/logo.png" type="image/x-icon" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.5/css/intlTelInput.min.css" integrity="sha512-yye/u0ehQsrVrfSd6biT17t39Rg9kNc+vENcCXZuMz2a+LWFGvXUnYuWUW6pbfYj1jcBb/C39UZw2ciQvwDDvg==" crossorigin="anonymous" />
    <link rel="stylesheet" href="assets/css/tagcloud.min.css">
    <link rel="stylesheet" href="assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/nprogress/0.2.0/nprogress.min.css" integrity="sha512-42kB9yDlYiCEfx2xVwq0q7hT4uf26FUgSIZBK8uiaEnTdShXjwr8Ip1V4xGJMg3mHkUt9nNuTDxunHF0/EgxLQ==" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/noty/3.1.4/noty.min.css" integrity="sha512-0p3K0H3S6Q4bEWZ/WmC94Tgit2ular2/n0ESdfEX8l172YyQj8re1Wu9s/HT9T/T2osUw5Gx/6pAZNk3UKbESw==" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css" integrity="sha512-1PKOgIY59xJ8Co8+NE6FZ+LOAZKjy+KY8iq0G4B3CyeY6wYHN3yt9PW0XpSriVlkMXe40PTKnXrLnZ9+fkDaog==" crossorigin="anonymous" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/js/all.min.js" integrity="sha512-YSdqvJoZr83hj76AIVdOcvLWYMWzy6sJyIMic2aQz5kh2bPTd9dzY3NtdeEAzPp/PhgZqr4aJObB3ym/vsItMg==" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="assets/css/main.css" />
    <style>
        .tc-tag{
            border-radius: 4px !important;
            font-size: 12px
        }
        .tc-selected{
            background: #A223B6 0% 0% no-repeat padding-box !important;
    color: #FFFFFF !important
        }
        #nprogress .bar{
    background-color: #A223B6 !important;
}

#nprogress .spinner-icon{
    border-left-color: #A223B6 !important;
    border-top-color: #A223B6 !important
}
    </style>
</head>

<body>
    <!--  -->
    <div class="container-fluid signup-page position-relative">
        <a href="https://useripple.com">
        <img src="assets/images/logo.png" alt="logo" class="img-fluid d-none d-lg-block d-xl-block"
            style="position: fixed; top: 2rem; right: 4rem;" />
        </a>
        <div class="row "> <!--align-items-center-->
            <div class="col-lg-5 col-xl-5 img-holder signup-leftside "></div>
            <!--
            <div class="col-lg-6 col-xl-5 img-holder signup-leftside ">
                <div class="bg"></div>
                <div class="info-holder">
                    <img src="assets/images/logo.png" alt="">
                </div>
            </div>-->
            <!--  -->
            <div class="col-lg-4 col-xl-4 offset-xl-1 offset-lg-1  signup-rightside">
                <div class="text-center">
                    <a href="https://useripple.com">
                    <img src="assets/images/logo.png" alt="logo" class="img-fluid d-xs-block d-sm-block d-md-block d-lg-none d-xl-none "
                        />
                    </a>
                </div>
                <h2 class="heading text-center text-lg-left mt-xl-5 mt-lg-5 mb-lg-5  mb-xl-5 mt-md-2  mt-sm-2  mt-xs-2  pt-md-0  pt-sm-0  pt-xs-0">Signup to partner with brands you love <i class="beating fa fa-heart" ></i> </h2>
                <!-- <p class="text text-center text-lg-left">Access to the most powerful tool in the <br /> entire design
                    and web industry.</p>
                <h4 class="title title-border mt-4 ">Register</h4>-->
                <form class="container-fluid mt-2 px-0" id="register" method="post">
                    {{ csrfField() }}
                    <div class="row">
                        <!--  -->
                        <div class="form-group col-md-6"> 
                            <label for="first_name">First Name</label>
                            <input type="first_name" required name="first_name" value="{{ flashMessages.get('first_name') || '' }}"   placeholder="First Name"class="form-input"/>
                            <p>{{ flashMessages.get('errors.first_name') || '' }}</p>
                        </div>
                        <!--  -->
                        <div class="form-group col-md-6">
                            <label for="last_name">Last Name</label>
                            <input type="last_name" required name="last_name" value="{{ flashMessages.get('last_name') || '' }}"   placeholder="Last Name"class="form-input"/>
                            <p>{{ flashMessages.get('errors.last_name') || '' }}</p>

                        </div>
                        <!--  -->
                        <div class="form-group col-md-6">
                            <label for="phone_number">Phone Number</label>
                            <input type="tel" required name="phone_number" value="+233" placeholder="Phone Number" id="phone_number" class="form-input" />
                            <p>{{ flashMessages.get('errors.phone_number') || '' }}</p>
                        </div>
                        <!--  -->
                        <div class="form-group col-md-6">
                            <label for="email">Email</label>
                            <input type="email" required name="email" value="{{ flashMessages.get('email') || '' }}" placeholder="E-mail Address" class="form-input" />
                            <p>{{ flashMessages.get('errors.email') || '' }}</p>
                        </div>
                        <!--  -->
                        <div class="form-group col-md-6">
                            <label for="country">Country</label>
                            <input type="country" disabled required name="country" id="country" value="Ghana (Gaana)"  placeholder="Country" class="form-input"/>
                            <p>{{ flashMessages.get('errors.country') || '' }}</p>
                        </div>
                        <!--  -->
                        <div class="form-group col-md-6">
                            <label for="city">City</label>
                            <input type="city" required name="city" value="{{ flashMessages.get('city') || '' }}" placeholder="City" class="form-input" />
                            <p>{{ flashMessages.get('errors.city') || '' }}</p>
                        </div>
                        <!--  -->
                        <div class="form-group col-12">
                            <label for="password">Password</label>
                            <input type="password" required name="password"  placeholder="Password" class="form-input" />
                            <p>{{ flashMessages.get('errors.password') || '' }}</p>
                        </div>
                        <!--  -->
                        <div class="form-group col-md-6">
                            <label for="instagram_handle">Instagram Handle</label>
                            <input type="instagram_handle" required name="instagram_handle" value="{{ flashMessages.get('instagram_handle') || '' }}" placeholder="[omit '@']" class="form-input instagram-input" />
                            <p>{{ flashMessages.get('errors.instagram_handle') || '' }}</p>
                        </div>
                        <!--  -->
                        <div class="form-group col-md-6">
                            <label for="twitter_handle">Twitter Handle</label>
                            <input type="twitter_handle" required name="twitter_handle" value="{{ flashMessages.get('twitter_handle') || '' }}"  placeholder="[omit '@']" class="form-input twitter-input"/>
                            <p>{{ flashMessages.get('errors.twitter_handle') || '' }}</p>
                        </div>
                        <!--  -->
                    </div>
                    <!--  -->
                    <h4 class="title mt-2">Interests</h4>

                    <select id="interests" multiple required>
                        @each(interest in interests)
                        <option value="{{interest.id}}" >{{interest.name}}</option>
                        @endeach
                    </select>
                    <!--  -->
                    <button type="submit" class="register-btn mt-2">Register</button>
                    <!--  -->
                </form>
                <!--  -->
            </div>
            <!--  -->
        </div>
        <!--  -->
    </div>
    <!--  -->


    <!-- JS Files -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.5/js/intlTelInput.min.js" integrity="sha512-P1duBYrnV48wVwdgs1SHIpqeFtNnez33h/bMdJOiAaK1A1Mu0NbgMtNm2AmDBh5PrkDiHoZ895B8aZsgnH894g==" crossorigin="anonymous"></script>
    <script src="assets/js/jquery-3.5.1.min.js"></script>
    <script src="assets/js/bootstrap.min.js"></script>
    <script src="assets/js/tagcloud.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/nprogress/0.2.0/nprogress.min.js" integrity="sha512-bUg5gaqBVaXIJNuebamJ6uex//mjxPk8kljQTdM1SwkNrQD7pjS+PerntUSD+QRWPNJ0tq54/x4zRV8bLrLhZg==" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/noty/3.1.4/noty.min.js" integrity="sha512-lOrm9FgT1LKOJRUXF3tp6QaMorJftUjowOWiDcG5GFZ/q7ukof19V0HKx/GWzXCdt9zYju3/KhBNdCLzK8b90Q==" crossorigin="anonymous"></script>
    <script src="assets/js/register-influencer.js"></script>
    <!-- JS Files End -->
